@model IEnumerable<WebBanHangOnline.Models.EF.Order>
@if (Model != null)
{
    var i = 1;
    <h2>Lich Su Mua Hang</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>STT</td>
                <td>Mã DH</td>
                <td>Giá</td>
                <td>Ngày mua</td>
                <td>Ngày duyệt</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@i</td>
                <td>@item.Code</td>
                <td>@item.Quantity</td>
                <td>@item.CreatedDate</td>
                <td>@item.ModifiedDate</td>

            </tr>
                i++;
            }
        </tbody>
    </table>
}
